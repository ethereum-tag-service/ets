{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [".env"],
  "tasks": {
    "build": {
      "env": [".env"],
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "env": [".env"],
      "cache": false
    },
    "test": {
      "env": [".env"]
    },
    "lint": {
      "outputs": []
    }
  }
}
