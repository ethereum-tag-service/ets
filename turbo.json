{
  "$schema": "https://turbo.build/schema.json",
  "globalDotEnv": [".env"],
  "pipeline": {
    "build": {
      "dotEnv": [".env"],
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "dotEnv": [".env"],
      "cache": false,
      "dependsOn": ["build"]
    },
    "test": {
      "dotEnv": [".env"],
      "dependsOn": ["build"]
    },
    "lint": {
      "outputs": [],
      "dependsOn": ["build"]
    }
  }
}
