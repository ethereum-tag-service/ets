# Required environment variables for staging Oracle deployment to AWS
# Make a copy of this file and rename it to .env.staging

# AWS Configuration 
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key

# Airnode Configuration
AIRNODE_STAGING_MNEMONIC=your-airnode-mnemonic-phrase-or-leave-empty-to-generate-one
HTTP_GATEWAY_API_KEY=your-api-key-or-leave-empty-to-generate-one
HEARTBEAT_API_KEY=your-heartbeat-key-or-leave-empty-to-generate-one
HEARTBEAT_URL=https://your-heartbeat-monitoring-endpoint.com/

# Admin Wallet (for transaction signing)
ETS_ADMIN_PRIVATE_KEY_STAGING=your-admin-private-key-with-funds-on-arbitrum-sepolia

# Network Configuration
ARBITRUM_SEPOLIA_URL=https://sepolia-rollup.arbitrum.io/rpc
STAGING_API_URL=https://api-staging.ets.xyz

# Testing
MAKE_TEST_REQUEST=false
TEST_TARGET_ID=0

# Logging
AIRNODE_LOG_LEVEL=INFO

# Optional - define these to use specific values
# STAGING_ENRICH_TARGET_ENDPOINT_ID=0x...
# STAGING_NEXT_AUCTION_ENDPOINT_ID=0x...