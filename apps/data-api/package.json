{
  "name": "@ethereum-tag-service/data-api",
  "author": "Ethereum Tag Service <team@ets.xyz>",
  "version": "0.0.1",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "clean-graph-node": "rm -rf graph-node/data/",
    "codegen": "graph codegen --output-dir src/generated",
    "deploy": "ts-node scripts/deploy.ts",
    "deploy:staging:base": "ts-node scripts/deploy.ts --config baseSepolia-staging",
    "deploy:staging:all": "pnpm deploy:staging:base",
    "deploy:production:base": "ts-node scripts/deploy.ts --config baseSepolia",
    "deploy:production:all": "pnpm deploy:production:base",
    "generate-yaml": "ts-node scripts/generate-yaml.ts",
    "generate-yaml:staging:base": "ts-node scripts/generate-yaml.ts --target baseSepolia --environment staging",
    "generate-yaml:production:base": "ts-node scripts/generate-yaml.ts --target baseSepolia --environment production",
    "start-graph-node": "cd scripts && ./start-graph-node.sh"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.80.0",
    "@graphprotocol/graph-ts": "^0.35.1",
    "@types/fs-extra": "^11.0.1",
    "@types/handlebars": "^4.1.0",
    "@types/minimist": "^1.2.2",
    "@types/node": "*",
    "axios": "^1.6.2",
    "fs-extra": "^11.1.1",
    "handlebars": "^4.7.7",
    "minimist": "^1.2.8",
    "node-fetch": "2",
    "npm": "^10.8.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.7.2"
  }
}
