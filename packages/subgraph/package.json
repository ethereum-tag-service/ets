{
  "name": "@ethereum-tag-service/subgraph",
  "author": "Ethereum Tag Service <team@ets.xyz>",
  "version": "0.0.1",
  "publishConfig": {
    "access": "public"
  },
  "type": "commonjs",
  "scripts": {
    "clean-graph-node": "rm -rf graph-node/data/",
    "codegen": "graph codegen --output-dir src/generated",
    "graph-deploy-local": "node scripts/deploy.js --deployment localhost",
    "graph-deploy-testnet-production": "node scripts/deploy.js --deployment testnet_production",
    "graph-deploy-testnet-stage": "node scripts/deploy.js --deployment testnet_stage",
    "remove-graph-node": "cd graph-node && docker-compose down && pnpm clean-graph-node",
    "prepare-local": "node scripts/generate-yaml.js --deployment localhost",
    "prepare-testnet-production": "node scripts/generate-yaml.js --deployment testnet_production",
    "prepare-testnet-stage": "node scripts/generate-yaml.js --deployment testnet_stage",
    "start-graph-node": "cd scripts && ./start-graph-node.sh",
    "test:build": "publint --strict"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.80.0",
    "@graphprotocol/graph-ts": "^0.35.1"
  },
  "devDependencies": {
    "axios": "^1.6.2",
    "fs-extra": "^11.1.1",
    "handlebars": "^4.7.7",
    "minimist": "^1.2.8"
  }
}
