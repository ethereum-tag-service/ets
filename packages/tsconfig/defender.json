{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Defender",
  "extends": "./base.json",
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "target": "es2017",
    "allowJs": true,
    "outDir": "dist",
    "rootDir": "src",
    "module": "es6",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true
  }
}