{
  "name": "@ethereum-tag-service/sdk-core",
  "author": "Ethereum Tag Service <team@ets.xyz>",
  "version": "0.0.3",
  "description": "Core SDK for Ethereum Tag Service",
  "publishConfig": {
    "access": "public"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "tsup src/index.ts --format cjs,esm --dts --outDir dist --watch",
    "build": "pnpm run clean && tsup src/index.ts --format cjs,esm --dts --outDir dist",
    "lint": "tsc",
    "test:build": "publint --strict"
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.12.7",
    "tsc-alias": "^1.8.8",
    "tsup": "^8.2.3",
    "typescript": "^5.4.5",
    "viem": "~2.4.0"
  },
  "dependencies": {
    "wagmi": "~2.5.7"
  }
}
